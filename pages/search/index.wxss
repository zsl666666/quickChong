.search-wrapper {
  width: 750rpx;
  background: #FAFAFA;
  height: 100vh;
  overflow-y: auto;
}
.search-test-style {
  background-color: #0D7AC2 !important;
  color: #fff !important;
  z-index: 222;
}
.van-nav-bar__arrow {
  color: #fff !important;
}
.index-search-wrapper {
  position: fixed;
  width: 750rpx;
  height: 180rpx;
  background: #FFFFFF;
  border-radius: 0rpx 0rpx 24rpx 24rpx;
  /* top: 170rpx; */
  left: 0;
  z-index: 100;
  overflow: hidden;
  box-shadow: 0rpx 1rpx 14rpx 0rpx rgba(0,0,0,0.1);
}
.van-nav-bar__text {
  color: #fff !important;
}
.index-search-limit {
  height: 106rpx;
}
.search-placeholder-div {
  height: 180rpx;
}
.home-top-search-wrapper {
  display: flex;
  justify-content: center;
  align-items: center;
}
.van-search--withaction {
  background: none !important;
  width: 570rpx;
  background: #F7F8FA;
}
.search-icon {
  display: inline-block;
  width: 80rpx;
  height: 80rpx;
  margin-left: 16rpx;
}
.charge-item-wrapper {
  width: 700rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 10rpx auto 0;
}
.charge-item {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 168rpx;
  height: 54rpx;
  background: #F6F8FA;
  border-radius: 47rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  text-align: center;
  line-height: 54rpx;
}
.charge-item-selected {
  background: rgba(13,122,194,0.08);
  border-radius: 47rpx;
  font-weight: 500;
  color: #0D7AC2;
}
.charge-img {
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  margin-right: 5rpx;
}
.checked-mark-wrapper {
  width: 100vw;
  /* height: 326rpx; */
  background: #FFFFFF;
}
.checked-mark-wrapper::after {
  display: block;
  content: '';
  margin: 0rpx auto;
  padding-bottom: 42rpx;
  width: 682rpx;
  height: 2rpx;
  background: #FFFFFF;
  box-shadow: inset 0rpx -1rpx 0rpx 0rpx rgba(0,0,0,0.05);
}
.mark-content-top {
  padding: 42rpx 34rpx 22rpx 34rpx;
  display: flex;
  align-items: center;
  /* height: max-content; */
}
.search-result-content-wrap {
  flex: 1;
  overflow: hidden;
}
.content-img {
  width: 182rpx;
  height: 182rpx;
  border-radius: 18rpx;
  overflow: hidden;
  margin-right: 18rpx;
}
.content-img image {
  display: inline-block;
  width: 182rpx;
  height: 182rpx;
}
.checked-content-title {
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(5,5,5,0.9);
}
