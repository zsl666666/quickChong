}
.device-site-type-b-box {
  width: 210rpx;
  height: 68rpx;
  background: #F7F8FA;
  border-radius: 34rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 68rpx;
  text-align: right;
  font-style: normal;
  color: rgba(0,0,0,0.8);
  text-align: center;
  margin-bottom: 20rpx;
}
.device-site-type-b-selected-box {
  background: #0D7AC2;
  font-weight: 600;
  color: #FFFFFF;
}
.device-type-b-box {
  box-sizing: border-box;
  width: 314rpx;
  height: 74rpx;
  border-radius: 37rpx;
  margin-bottom: 24rpx;  
  display: flex;
  align-items: center;
  padding: 10rpx;
  font-family: PingFangSC, PingFang SC;
  font-size: 28rpx;
  text-align: left;
  font-style: normal;
  background: #F7F8FA;
  color: rgba(0,0,0,0.8);
  font-weight: 400;
}
.device-type-b-selected-box {
  background: #0D7AC2;
  color: #FFFFFF;
  font-weight: 600;
}
.device-type-b-box-type-icon {
  width: 54rpx;
  height: 54rpx;
  border-radius: 50%;
}
.device-type-b-box-type-title {
  flex: 1;
  text-align: center;
}

