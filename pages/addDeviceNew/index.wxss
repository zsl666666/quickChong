.device-page-wrapper {
  box-sizing: border-box;
  width: 100vw;
  min-height: 100vh;
  background: #F4F4F4;
  overflow-y: auto;
}
.add-desc-wrap {
  padding: 23rpx 22rpx;
  background: rgba(26,146,225,0.12);
  border-radius: 18rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 500;
  font-size: 26rpx;
  color: #1A92E1;
  text-align: justify;
  font-style: normal;
  display: flex;
  align-items: center;
  margin: 18rpx 16rpx;
}
.add-desc-wrap-icon {
  width: 36rpx;
  height: 36rpx;
  margin-right: 13rpx;
}

.every-device-chunk {
  width: 714rpx;
  background: #FFFFFF;
  border-radius: 18rpx;
  margin: 18rpx auto 18rpx;
}
.device-chunk-top-wrap {
  padding: 34rpx 0 30rpx 28rpx;
}
.device-chunk-top {
  display: flex;
  align-items: center;
  padding: 34rpx 0 30rpx 28rpx;
}
.device-chunk-top-icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.device-chunk-top-title {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0,0,0,0.6);
}
.device-chunk-top-content-desc {
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  font-size: 22rpx;
  color: rgba(0,0,0,0.2);
  line-height: 30rpx;
  text-align: left;
  font-style: normal;
  margin-top: 8rpx;
}

.device-type-bottom-wrap {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding-left: 28rpx;
  padding-right: 28rpx;
  padding-bottom: 10rpx;
}
.device-site-type-bottom-wrap {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  box-sizing: border-box;
  padding-left: 28rpx;
  padding-right: 28rpx;
