  font-size: 28rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  color: #000000;
  line-height: 38rpx;
}
.device-desicribtion-img .device-img-label-class {
  font-size: 22rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  color: #A0A3AC;
  line-height: 30rpx;
}
.device-img-title-desc, .device-img-label-class {
  font-size: 22rpx;
  font-family: PingFangSC, PingFang SC;
