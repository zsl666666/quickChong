/* pages/addDevice/index.wxss */
.device-page-wrapper {
  width: 100vw;
  /* height: 100vh; */
  min-height: 100vh;
  background: #F4F4F4;
  overflow-y: auto;
}
.device-excharge-type-wrapper {
  width: 714rpx;
  height: 204rpx;
  background: #FFFFFF;
  border-radius: 18rpx;
  margin: 18rpx auto 14rpx;
}
.excharge-type-title {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0,0,0,0.6);
  padding: 34rpx 0 30rpx 28rpx;
}
.excharge-type-data-wrapper {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 0 28rpx 34rpx 28rpx;
}
.excharge-type-item {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 206rpx;
  height: 68rpx;
  background: #F7F8FA;
  border-radius: 34rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0,0,0,0.8);
}
.excharge-type-icon {
  display: inline-block;
  height: 30rpx;
  width: 30rpx;
  margin-right: 5rpx;
}
.excharge-type-item-select {
  background: #0D7AC2;
  font-weight: 600;
  color: #FFFFFF;
}
.device-excharge-content {
  width: 714rpx;
  /* height: 480rpx; */
  background: #FFFFFF;
  border-radius: 18rpx;
  margin: 0rpx auto 14rpx;
}
.cell-group-content {
  padding: 28rpx 0rpx;
}
.device-input-style {
  height: 100rpx;
}
.cell-title-style {
  color: var(--field-label-color,#646566);
}
.device-exhcarge-content {
  height: 670rpx;
}
.device-type-wrapper {
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-bottom: 80rpx;
}
.device-type-item {
  width: 300rpx;
  height: 68rpx;
  background: #F7F8FA;
  border-radius: 34rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0,0,0,0.8);
}
.device-type-item-selected {
  background: #0D7AC2;
  font-weight: 600;
  color: #FFFFFF;
}
.device-desicribtion {
  height: 340rpx;
  margin-bottom: 160rpx;
  /* padding-top: 10rpx; */
}
.text-area-style{
  height: 100rpx !important;
}
.device-desicribtion-img {
  /* min-height: 352rpx; */
  /* margin-bottom: 160rpx; */
  padding-top: 10rpx;
}
.van-uploader {
  margin-left: 28rpx;
}
.device-bottom-submit-wrapper {
  background: #fff;
  height: 120rpx;
  width: 100vw;
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 10;
}
.submit-btn {
  height: 80rpx;
  width: 600rpx;
  background: #0D7AC2;
  border-radius: 34rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  line-height: 80rpx;
  color: #FFFFFF;
  text-align: center;
}
.submit-btn-disabled {
  background: #0D7AC2;
  opacity: 0.3;
}
.add-device-addAddress {
  width: 700rpx;
  margin: 0 auto;
  margin-top: 10rpx;
}
.van-search__action {
  margin-left: 10rpx;
}
.address-list-wrapper {
  width: 680rpx;
  margin: 0 auto;
  margin-bottom: 10rpx;
  border-bottom: 1px solid #eee;
  padding-bottom: 10rpx;
}
.address-title {
  font-size: 26rpx;
}
.address-detail {
  color: #999;
  font-size: 22rpx;
  position: relative;
  left: 33rpx;
}
.address-title-content {
  margin-left: 10rpx;
}
.address-loading {
  margin-left: 48%;
}
.title-color {
  color: #0D7AC2;
}
.process-wrapper {
  position: fixed;
  display: flex;
  width: 100vw;
  height: 100vh;
  top: 0;
  left: 0;
  align-items: center;
  justify-content: center;
}
.radio-custom {
  margin-left: 20rpx;
}
.title-class {
  color: var(--field-label-color,#646566);
}
.radio-text {
  font-size: 25rpx;
}

.monitor-value-style {
  color: var(--field-input-text-color,#323233) !important;
}

.box-wrap {
  padding: 32rpx 28rpx;
}
.add-device-type-title {
  height: 38rpx;
  font-size: 28rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  color: #000000;
  line-height: 38rpx;
  /* padding: 34rpx 0 0rpx 28rpx; */
}
.custom-required-ident::before {
  color: #ee0a24;
  content: "*";
  font-size: 28rpx;
  margin-left: -12rpx;
}
.map-location-desc {
  height: 30rpx;
  font-size: 22rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  color: rgba(0,0,0,0.2);
  line-height: 30rpx;
}
.map-wrap {
  width: 658rpx;
  height: 250rpx;
  margin: 30rpx auto;
}
.expansion-packup {
  box-sizing: border-box;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0,0,0,0.6);
  padding: 34rpx 28rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.is-not-expansion {
  margin-bottom: 160rpx;
}
.expansion-title {
  color: red;
  height: 38rpx;
  font-size: 28rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  color: rgba(0,0,0,0.6);
  line-height: 38rpx;
}
.expansion-desc {
  font-size: 22rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  color: rgba(0,0,0,0.2);
  line-height: 30rpx;
}

