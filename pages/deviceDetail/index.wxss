/* pages/deviceDetail/index.wxss */
.device-detail {
  height: 100vh;
  background: #F4F4F4;
  overflow: hidden;
}

.constom-device-detail-nav-bar .van-nav-bar {
  z-index: 100;
  background: none;
  border: none;
}
.constom-device-detail-nav-bar .van-nav-bar::after {
  /* border: none; */
  opacity: 0;
}
.detail-nav-bar-left {
  padding-right: 30rpx;
}


.top-device-image {
  width: 100%;
  height: 454rpx;
  background: rgba(22,87,129,0.7);
  backdrop-filter: blur(14px);
  background-image: url(data:image/jpeg;base64,);
  background-size: cover;
  background-repeat: no-repeat;
}
.top-device-image-upload {
  position: absolute;
  top: var(--top);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  font-size: 28rpx;
  color: rgba(255,255,255,0.9);
  line-height: 40rpx;
  margin: auto;
  left: 50%;
  transform: translateX(-50%);
}
.top-device-image-upload-icon {
  width: 114rpx;
  height: 114rpx;
  margin-bottom: 16rpx;
  margin-top: 4rpx;
}

.device-detail-content {
  position: relative;
  margin-top: -36rpx;
  z-index: 2;
  overflow-y: auto;
  height: calc(100vh - 400rpx - 170rpx);
  width: max-content;
  margin-left: auto;
  margin-right: auto;
  border-radius: 24rpx 24rpx 0rpx 0rpx;
}

.device-detail-content-wrap {
  box-sizing: border-box;
  width: 726rpx;
  background: #FFFFFF;
  border-radius: 24rpx;
  padding: 24rpx;
  margin: auto;
  margin-bottom: 18rpx;
}

.device-main-info {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.device-main-info-charging-type-title {
  font-family: PingFangSC, PingFang SC;
  font-weight: 500;
  font-size: 42rpx;
  color: rgba(0,0,0,0.8);
  line-height: 60rpx;
  text-align: left;
  font-style: normal;
  margin-bottom: 8rpx;
}
.device-main-info-charging-type-evaluate {
  display: flex;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  font-size: 26rpx;
  color: rgba(5,5,5,0.4);
  line-height: 34rpx;
}
.device-main-info-charging-type-mark {
  font-family: PingFangSC, PingFang SC;
  font-weight: 500;
  font-size: 32rpx;
  color: #0D7AC2;
  line-height: 34rpx;
}
.device-main-info-charging-type-line {
  width: 1rpx;
  height: 18rpx;
  background: rgba(0,0,0,0.4);
  margin-left: 14rpx;
  margin-right: 14rpx;
  margin-top: 10rpx;
}
.device-main-info-go-clock {
  width: 136rpx;
  height: 68rpx;
  border-radius: 47rpx;
  border: 2rpx solid #0D7AC2;
  line-height: 68rpx;
  text-align: center;
  font-family: PingFangSC, PingFang SC;
  font-weight: 500;
  font-size: 28rpx;
  color: #0D7AC2;
}

.operate-detail {
  display: flex;
